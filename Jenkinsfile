pipeline{
    agent any
     stages{
         stage("Hello"){
             steps{
                 echo "hello world"
             }
         }
         stage("Build"){
             steps{
                echo "this is build stage"
             }
         }
         stage("Test"){
             steps{
                 echo "this is test stage"
             }
         }
         stage("Deploy"){
             steps{
                 echo "this is deploy stage"
             }
         }
     }
}
